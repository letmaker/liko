{
  "name": "liko",
  "version": "1.1.31",
  "author": "handler",
  "description": "liko is a 2D physics engine written in TypeScript",
  "keywords": ["2d", "physics", "engine", "webgpu"],
  "main": "dist/liko.cjs.js",
  "module": "dist/liko.es.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "README.md", "LICENSE"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/letmaker/liko.git"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:browser": "vitest --workspace=vitest.workspace.ts",
    "doc": "typedoc --options typedoc-html.json",
    "markdown": "typedoc --options typedoc-md.json",
    "md": "npm run markdown && node scripts/merge-docs.js"
  },
  "dependencies": {
    "@webgpu/types": "^0.1.60",
    "planck": "^1.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@vitest/browser": "^3.1.2",
    "playwright": "^1.52.0",
    "typedoc": "^0.28.3",
    "typedoc-plugin-markdown": "^4.7.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.4",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.2"
  }
}
